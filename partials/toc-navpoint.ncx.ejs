<navPoint class="<%= item.navclass %>" id="<%= item.id %>" playOrder="<%= item.spineorder%>">
    <navLabel><text><%= item.title %></text></navLabel>
    <content src="<%= item.href %>" />
    <%
    if (tocItem.hasOwnProperty("subtoc") && tocItem.subtoc) {
        tocItem.subtoc.forEach(function(subtocItem) {
            var subitem = akashacms.plugin('akashacms-epub').findManifestItem(subtocItem.id);
            %>
            <%- partial('toc-navpoint.ncx.ejs', { item: subitem, tocItem: subtocItem }) %>
            <%
        });
    }
    %>
</navPoint>